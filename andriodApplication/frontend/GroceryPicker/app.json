{
  "expo": {
    "name": "GroceryPicker",
    "slug": "app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "grocerypicker",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.ckwflash.GroceryPicker",
      "softwareKeyboardLayoutMode": "pan",
      "intentFilters": [
        {
          "action": "VIEW",
          "autoVerify": true,
          "data": [
            {
              "scheme": "https",
              "host": "grocerypicker.app",
              "pathPrefix": "/auth/ResetPassword"
            },
            {
              "scheme": "grocerypicker",
              "host": "auth/ResetPassword"
            }
          ],
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        }
      ],
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyAn5u8hq2qvuwdAq42cAhkLkkm6gLT4yLI"
        }
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/icon.png",
      "softwareKeyboardLayoutMode": "pan"
    },
    "plugins": [
      "expo-router",
      [
        "expo-maps",
        {
          "requestLocationPermission": true,
          "locationPermission": "Allow GroceryPicker to use your location"
        },
        "expo-splash-screen",
        {
          "image": "./assets/images/icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      "expo-font"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {

        "projectId": "76bbf570-3756-4bbe-af52-c10992148dbd"

      }
    },
    "owner": "grocery-picker"
  }
}
